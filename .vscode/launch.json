    "version": "0.2.0",
    "configurations": [
        {	
            "type": "node",	
            "request": "attach",	
            "name": "server",	
            "processId": "${command:PickProcess}",	
            "protocol": "inspector",	
            "restart": true	
        },
        {
            // node init --diretorio=/home/foo/Documents --projeto=foo-estatico,foo-api --autor=diegomdrs --task=1111111 --mostrar-num-modificacao --mostrar-deletados
            "type": "node",
            "request": "launch",
            "name": "home server gerador.js",
            "program": "${workspaceFolder}/init.js",
            "args": [
                "--server"
            ]
        },
        {
            // node init --diretorio=/home/foo/Documents --projeto=foo-estatico,foo-api --autor=diegomdrs --task=1111111 --mostrar-num-modificacao --mostrar-deletados
            "type": "node",
            "request": "launch",
            "name": "home gerador.js",
            "program": "${workspaceFolder}/lib/gerador.js",
            "args": [
                "--diretorio=/home/foo/Documents",
                "--projeto=foo-estatico,foo-api",
                "--autor=diegomdrs",
                "--task=1111111",
